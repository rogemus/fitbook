{
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "curly": 2,
    "eqeqeq": 2,
    "no-extend-native": 2,
    "no-proto": 2,
    "no-caller": 2,
    "no-unused-vars": [2, { "vars": "all", "args": "none" }],
    "indent": [ 2, "tab", {"SwitchCase": 1}],
    "new-cap": 2,
    "quotes": [2, "single"],
    "max-statements": [2, 45],
    "max-len": [2, 150],
    "no-eq-null": 2,
    "operator-linebreak": 2,
    "no-multiple-empty-lines": [2, { max: 2 }],
    "space-unary-ops": 2,
    "no-multi-spaces": 2,
    "keyword-spacing": 2,
    "space-infix-ops": 2,
    "comma-spacing": [2, { "before": false, "after": true }],
    "no-trailing-spaces": 2,
    "comma-dangle": 2,
    "wrap-iife": 2,
    "no-extra-semi": 2,
    "semi-spacing": 2,
    "semi": [2, "always"],
    "spaced-comment": 2,
    "block-scoped-var": 2,
    "global-require": 2,
    "no-mixed-requires": 2,
    "no-new-require": 2,
    "arrow-spacing": 2,
    "no-const-assign": 2,
    "no-var": 2,
    "prefer-const": 2,
    "jsx-quotes": [2, "prefer-double"],
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
  }
}